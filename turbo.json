{
    "$schema": "https://turborepo.org/schema.json",
    "remoteCache": {
        "signature": true
    },
    "pipeline": {
        "dev": {
            "cache": false
        },
        "build": {
            "cache": true,
            "outputs": [
                "dist/**",
                "storybook-static/**"
            ],
            "dependsOn": [
                "^build"
            ]
        }
    }
}